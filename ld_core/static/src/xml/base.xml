<?xml version="1.0" encoding="UTF-8"?>
<!-- vim:fdl=1: -->
<templates id="template" xml:space="preserve">
<t t-extend="Login">
    <t t-jquery="img" t-operation="replace">
    <img src='/ld_core/static/src/img/ld_logo.png'
    width="180px" alt="LOGO"/>
    </t>
     <t t-jquery="div.oe_login_footer" t-operation="after">
            <div class="oe_login_footer" style="top:250px">
                <a href="http://www.ld.org/" target="_blank">Provided by <span>ld Inc</span></a>
            </div>
     </t>
</t>
<t t-extend="WebClient">
    <t t-jquery="span.url" t-operation="replace">
        <span class="url"><a id="support_link" href="http://www.ld.org/support" target="_blank"></a></span>
    </t>
</t>
</templates>
