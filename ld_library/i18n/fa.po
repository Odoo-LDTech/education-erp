#. module: ld_library
#: model:mail.template,body_html:ld_library.email_template_media_movement_after
msgid ""
"\n"
"            <p>Dear <strong> ${object.student_id.name} </strong>\n"
"            <p>Your ${object.media_type_id.name}, <strong> ${object.media_id.name} </strong> has been expired. Please return it as soon as possible.</p>\n"
"            <p>Thank you.</p>\n"
"            "
msgstr ""
"\n"
"            <p> عزیز <strong> $ {object.student_id.name} </strong>\n"
"            <p> $ {object.media_type_id.name} ، <strong> $ {object.media_id.name} </strong> منقضی شده است. لطفاً در اسرع وقت آن را برگردانید. </p>\n"
"            <p> متشکرم. </p>\n"
"            "

#. module: ld_library
#: model:mail.template,body_html:ld_library.email_template_media_movement
msgid ""
"\n"
"            <p>Dear <strong> ${object.student_id.name} </strong>\n"
"            <p>Your ${object.media_type_id.name}, <strong> ${object.media_id.name} </strong> will \n"
"                % if object.get_diff_day() == 0:\n"
"                    be expiring <strong>today.</strong>\n"
"                % else:\n"
"                    expire in <strong> ${object.get_diff_day()}</strong> day(s).\n"
"                % endif\n"
"                Please return it as soon as possible.</p>\n"
"            <p>Thank you.</p>\n"
"            "
msgstr ""
"\n"
"            <p> عزیز <strong> $ {object.student_id.name} </strong>\n"
"            <p> $ {object.media_type_id.name} ، <strong> $ {object.media_id.name} </strong>\n"
"                ٪ اگر object.get_diff_day () == 0:\n"
"                    امروز در حال انقضا </strong> است. </strong>\n"
"                ٪ دیگر:\n"
"                    در <strong> $ {object.get_diff_day ()} </strong> روز (ها) منقضی شوید.\n"
"                ٪ endif\n"
"                لطفاً در اسرع وقت آن را برگردانید. </p>\n"
"            <p> متشکرم. </p>\n"
"            "

