                   t-thumbnail="web_ld/static/src/img/snippets_icons/op_educat.png"/>
                <t t-snippet="web_ld.achievement"
                   t-thumbnail="web_ld/static/src/img/snippets_icons/op_educat.png"/>
                <t t-snippet="web_ld.teacher"
                   t-thumbnail="web_ld/static/src/img/snippets_icons/op_educat.png"/>
                <t t-snippet="web_ld.event"
                   t-thumbnail="web_ld/static/src/img/snippets_icons/op_educat.png"/>
                <t t-snippet="web_ld.newsfeed"
                   t-thumbnail="web_ld/static/src/img/snippets_icons/op_educat.png"/>
                <t t-snippet="web_ld.footer"
                   t-thumbnail="web_ld/static/src/img/snippets_icons/op_educat.png"/>
            </div>
        </xpath>
    </template>
</odoo>
